<script language="JavaScript">
function tryDownload()
{
	var x;
	var varcount=0;
	var problems=0;
	var varstr="";
	var varlist="";
	var varstrX="";
	var sqlstr="";
		
	//Added 11/01/11
	var SWService=0;

	if (0 == 0)
	{
		x = document.form1.VarName.length;
		var j=0;
		for (var i=0; i < x; i++)
		{
			j=i;
			if (document.form1.VarName[j].checked)
			{
				if (varstr<="")
				{
					varlist=document.form1.VarName[j].title;
					varstr=document.form1.VarName[j].value;
					varstr2= document.form1.VarName[j].value;
				}
				else
				{
					varstr=varstr + ',' + document.form1.VarName[j].value;
					varlist=varlist + ',' + document.form1.VarName[j].title;
					varstr2=varstr2 + ',' + document.form1.VarName[j].value;
				}
				varcount=varcount+1;
				
				//Added 11/01/11
				if (document.form1.VarName[j].value=='SERVICE_CLASS')
				{
					SWService=1;
				}
			}
		}
	
		if (varcount<=0)
		{
			problems=1;
			alert("You have not selected any variables. Please try again");
		}
	}
	else
	{
		if (document.form1.VarName.checked)
		{
			if (varstr<="")
			{
				varlist=document.form1.VarName.title;
				varstr=document.form1.VarName.value;
				varstr2= document.form1.VarName.value;
			}
			else
			{
				varstr=varstr + ',' + document.form1.VarName.value;
				varlist=varlist + ',' + document.form1.VarName.title;
				varstr2=varstr2 + ',' + document.form1.VarName.value;
			}
		}
		else
		{
			problems=1;
			alert("You have not selected any variables. Please try again");
		}
		
	}
	var geovalue=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].value;
	var timevalue=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].value;
	var yearvalue=document.form1.XYEAR.options[document.form1.XYEAR.selectedIndex].value;
	var geotext=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].text;
	var timetext=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].text;
	var wheregeo="";
	var wheretime="";

	if (geovalue=="All")
	{
		wheregeo="";
	}
	else
	{
		if (2==1)
		{
			wheregeo=" WHERE ( ORIGIN_STATE_NM ='" + geotext + "' )  ";
		}
		else if (2==2)
		{
			wheregeo=" WHERE ( DEST_STATE_NM='" + geotext + "' OR ORIGIN_STATE_NM ='" + geotext + "' ) ";
		}
	}

	if (timevalue=="All")
	{
		wheretime="";
	}
	else
	{
		if (wheregeo=="")
		{
			wheretime=" WHERE Month =" + timevalue ;
		}
		else
		{
			wheretime=" AND Month =" + timevalue ;
		}
	}
	if (yearvalue=="All")
	{
		whereyear="";
	}
	else
	{
		if ((wheretime=="") && (wheregeo==""))
		{
			whereyear=" WHERE YEAR=" + document.form1.XYEAR.options[document.form1.XYEAR.selectedIndex].text;
		}
		else
		{
	 		whereyear=" AND YEAR=" + document.form1.XYEAR.options[document.form1.XYEAR.selectedIndex].text;
		}
	}
	if (problems<=0)
	{
		//sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear ;
		if ((236 == 264) || (236 == 254))  //Schedule T1 & T2
		{
			if (SWService==1)    // SERVICE_CLASS selected
			{
				if (236 == 254)  // T2
				{
					//var mystr = "wheregeo ==> " + wheregeo.length + ", wheretime ==> " + wheretime.length + ", whereyear ==> " + whereyear.length ;
					//alert(mystr);
					if ((wheregeo.length==0) && (wheretime.length==0) && (whereyear.length==0))
					{
						sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value + " WHERE Service_Class NOT IN ('K', 'V')";
						//alert("*cond 1==> " + sqlstr);
					}
					else
					{
						sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear + " AND Service_Class NOT IN ('K', 'V')";
						//alert("**cond 2==> " + sqlstr);
					}
				}
				else  // T1
				{
					//var mystr2 = "wheregeo ==> " + wheregeo.length + ", wheretime ==> " + wheretime.length + ", whereyear ==> " + whereyear.length ;
					//alert(mystr2);
					if ((wheregeo.length==0) && (wheretime.length==0) && (whereyear.length==0))
					{
						sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  " WHERE  Service_Class NOT IN ('K', 'V' , 'Z')";
						//alert("*cond 1==> " + sqlstr);
					}
					else
					{
						sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear + "  AND Service_Class NOT IN ('K', 'V' , 'Z')";
						//alert("**cond 2==> " + sqlstr);
					}
				}
				//alert(sqlstr);
			}
			else
			{
				sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear ;
			}
		}
		else
		{
			sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear ;
		}
		document.form1.sqlstr.value=sqlstr;
		document.form1.geo.value=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].text;
		document.form1.time.value=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].text;
		document.form1.varlist.value=varstr;

		document.form1.submit();
	}
	else
	{
		document.form1.reset();
	}
}
function tryDownload2()
{
	var timevalue=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].value;
	var yearvalue=document.form1.XYEAR.options[document.form1.XYEAR.selectedIndex].value;
	var db_short_name=document.form1.DBShortName.value;
	var table_name=document.form1.UserTableName.value;
	var zip_filename = db_short_name + "_" + table_name + "_"  + yearvalue + "_" + timevalue + ".zip";
	if ((yearvalue==2015) && (timevalue > 12))
	{
		alert("Requested data do not exist.");
	}
	else
	{
		//location.replace("/Download/"+zip_filename);
		//location.replace("http://tsdata-test.dot.gov/PREZIP/"  +zip_filename);
	
		var server = window.location.hostname;
		var DL_URL="";
		
		if (server.toUpperCase() == "TRANSTATS-DEV.DOT.GOV")
		{
			DL_URL = "http://tsdata-dev.dot.gov";		
		}
		else if (server.toUpperCase() == "TRANSTATS-TEST.DOT.GOV")
		{
			DL_URL = "http://tsdata-test.dot.gov";
		}
		else if (server.toUpperCase() == "WWW.TRANSTATS.BTS.GOV")
		{
			DL_URL = "http://tsdata.bts.gov";
		}
		location.replace( DL_URL + "/PREZIP/"  + zip_filename);
		

	}

}
function tryDownload3()
{
	var varcount=0;
	var problems=0;
	var varstr="";
	var vString=document.form1.GroupName[0].value;
	var check=vString.charAt(vString.length-1);
	
	if (check==0)
	{
		alert("You have not selected any ID variables and won't be able to use the data. Please try again.");
	}
	else if (document.form1.MergeSub.checked && document.form1.AllGroups.checked && document.form1.filter2.title.length>0)
	{
		msg="You may have selected more than 2000 variables for one table. " +
					"The table is too wide. " +
					"Please deselect Merge data groups and try again.";
		alert(msg);
	}
	else
	{
		var x=document.form1.GroupName.length;
		if (document.form1.MergeSub.checked)
		{
			for (var i=0; i<x; i++)
			{
				vString=document.form1.GroupName[i].value;
				check=vString.charAt(vString.length-1);
				var groupName=vString.substring(0,vString.length-1);
				var boxName=vString.substring(0,vString.length-1);
				if (check>1)
				{
					if (document.form1[boxName])
					{
						var y = document.form1[boxName].length;
						if (y)
						{
							for (var j=0;j<y;j++)
							{
								if (document.form1[boxName][j].checked)
								{
									if (varstr<="")
									{
										varstr=document.form1[boxName][j].value;
									}
									else
									{
										varstr=varstr + ',' + document.form1[boxName][j].value;
									}
									if (i>0)
									{
										varcount=varcount+1;
									}
								}
							}
						}
						else
						{
							if (document.form1[boxName].checked)
							{
								if (varstr<="")
								{
									varstr=document.form1[boxName].value;
								}
								else
								{
									varstr=varstr + ',' + document.form1[boxName].value;
								}
								varcount=varcount+1;
							}
						}
					}
					else
					{
						if (varstr<="")
						{
							varstr=groupName;
						}
						else
						{
							varstr=varstr + ',' + groupName;
						}
						if (i>0)
						{
							varcount=varcount+1;
						}
					}
				}
				else if (check==1)
				{
					if (document.form1[boxName])
					{
						var y = document.form1[boxName].length;
						if (y)
						{
							for (var j=0;j<y;j++)
							{
								if (varstr<="")
								{
									varstr=document.form1[boxName][j].value;
								}
								else
								{
									varstr=varstr + ',' + document.form1[boxName][j].value;
								}
								if (i>0)
								{
									varcount=varcount+1;
								}
							}
						}
						else
						{
							if (varstr<="")
							{
								varstr=document.form1[boxName].value;
							}
							else
							{
								varstr=varstr + ',' + document.form1[boxName].value;
							}
							varcount=varcount+1;
						}
					}
					else
					{
						if (varstr<="")
						{
							varstr=groupName;
						}
						else
						{
							varstr=varstr + ',' + groupName;
						}
						if (i>0)
						{
							varcount=varcount+1;
						}
					}
				}
			}
			if (varcount<=0)
			{
				if (confirm("You have only selected ID variables. Do you want to continue?"))
				{
					document.form1.sqlstr.value=1;
					document.form1.varlist.value=varstr;
					//alert(varstr);
					document.form1.submit();
				}
			}
			else if (varcount>2000)
			{
				msg="You have selected more than 2000 variables for one table. " +
					"The table is too wide. " +
					"Please deselect Merge data groups and try again.";
				alert(msg);
			}
			else
			{

				if (document.form1.SUMLEVEL){
					document.form1.sqlstr.value=1;
					document.form1.geo.value=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].value;
					document.form1.time.value=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].text;
					document.form1.varlist.value=varstr;
					document.form1.submit();
				}else{
					var geovalue=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].value;
					var timevalue=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].value;
					var yearvalue=document.form1.XYEAR.options[document.form1.XYEAR.selectedIndex].value;
					var geotext=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].text;
					var timetext=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].text;
					var wheregeo="";
					var wheretime="";

					if (geovalue=="All")
					{
						wheregeo="";
					}
					else
					{
						if (2==1)
						{
							wheregeo=" WHERE ( ORIGIN_STATE_NM ='" + geotext + "' )  ";
						}
						else if (2==2)
						{
							wheregeo=" WHERE ( DEST_STATE_NM='" + geotext + "' OR ORIGIN_STATE_NM ='" + geotext + "' ) ";
						}
					}

				if (timevalue=="All")
				{
					wheretime="";
				}
				else
				{
					if (wheregeo=="")
					{
						wheretime=" WHERE Month =" + timevalue ;
					}
					else
					{
						wheretime=" AND Month =" + timevalue ;
					}
				}
				if (yearvalue=="All")
				{
					whereyear="";
				}
				else
				{
					if ((wheretime=="") && (wheregeo==""))
					{
						whereyear=" WHERE YEAR=" + document.form1.XYEAR.options[document.form1.XYEAR.selectedIndex].text;
					}
					else
					{
				 		whereyear=" AND YEAR=" + document.form1.XYEAR.options[document.form1.XYEAR.selectedIndex].text;
					}
				}
				if ((236 == 264) || (236 == 254))  // Schedule T1 & T2
				{
					if (236 == 254)  // T2
					{
						if ((wheregeo.length==0) && (wheretime.length==0) && (whereyear.length==0))
						{
							sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value + " WHERE Service_Class NOT IN ('K', 'V')";
						}
						else
						{
							sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear + " AND Service_Class NOT IN ('K', 'V')";
						}
					}
					else  // T1
					{
						if ((wheregeo.length==0) && (wheretime.length==0) && (whereyear.length==0))
						{
							sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  "  WHERE Service_Class NOT IN ('K', 'V' , 'Z')";
						}
						else
						{
							sqlstr=" SELECT " + varstr2 + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear + " AND Service_Class NOT IN ('K', 'V' , 'Z')";
						}
					}
					
				}
				else
				{
					sqlstr=" SELECT " + varstr + " FROM  " + document.form1.RawDataTable.value +  wheregeo + wheretime + whereyear ;
				}
				document.form1.sqlstr.value=sqlstr;
				document.form1.geo.value=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].value;
				document.form1.time.value=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].text;
				document.form1.varlist.value=varstr;
				//alert(sqlstr);
				document.form1.submit();
				}
			}
		}
		else		//Separate data groups
		{
			var varstrAll="";
			var groupstr="";
			var vString=document.form1.GroupName[0].value;
			var boxName=vString.substring(0,vString.length-1);      //Get group ID
			var y = document.form1[boxName].length;
			if (y)
			{
				for (var j=0;j<y;j++)
				{
					if ((document.form1.GroupName[0].checked)||(document.form1[boxName][j].checked))
					{
						varstrAll=varstrAll + document.form1[boxName][j].value + ",";
					}
				}
			}
			else
			{
				if ((document.form1.GroupName[0].checked)||(document.form1[boxName].checked))
				{
					varstrAll=varstrAll + document.form1[boxName].value + ",";
				}
			}

			for (var i=1; i<x; i++)  //Add one group at a time
			{
				vString=document.form1.GroupName[i].value;
				var boxName=vString.substring(0,vString.length-1);
				check=vString.charAt(vString.length-1)
				if (check>1)
				{
					varstr=varstr + varstrAll;
					groupstr=groupstr + boxName + ";";
					if (document.form1[boxName])
					{
						var y = document.form1[boxName].length;
						if (y)
						{
							for (var j=0;j<y;j++)
							{
								if (document.form1[boxName][j].checked)
								{
									varstr=varstr +  document.form1[boxName][j].value + ",";
									varcount=varcount+1;
								}
							}
						}
						else
						{
							if (document.form1[boxName].checked)
							{
								varstr=varstr +  document.form1[boxName].value + ",";
								varcount=varcount+1;
							}
						}
						varstr=varstr.substr(0,varstr.length-1) + ";";
					}
					else
					{
						varstr=varstr +  boxName + ";";
						varcount=varcount+1;
					}
				}
				else if (check==1)
				{
					varstr=varstr + varstrAll;
					groupstr=groupstr + boxName + ";";
					if (document.form1[boxName])
					{
						var y = document.form1[boxName].length;
						if (y)
						{
							for (var j=0;j<y;j++)
							{
									varstr=varstr +  document.form1[boxName][j].value + ",";
									varcount=varcount+1;
							}
						}
						else
						{
							varstr=varstr +  document.form1[boxName].value + ",";
							varcount=varcount+1;
						}
						varstr=varstr.substr(0,varstr.length-1) + ";";
					}
					else
					{
						varstr=varstr +  boxName + ";";
						varcount=varcount+1;
					}
				}
			}
			document.form1.sqlstr.value=0;
			document.form1.geo.value=document.form1.GEOGRAPHY.options[document.form1.GEOGRAPHY.selectedIndex].value;
			document.form1.time.value=document.form1.FREQUENCY.options[document.form1.FREQUENCY.selectedIndex].text;
			document.form1.varlist.value=varstr;
			document.form1.grouplist.value=groupstr;
			if (varcount==0)
			{
				if (confirm("You have only selected ID variables. Do you want to continue?"))
				{
					document.form1.sqlstr.value=1;
					document.form1.varlist.value=varstrAll.substr(0,varstrAll.length-1);
					//alert(varstrAll.substr(0,varstrAll.length-1));
					document.form1.submit();
				}
			}
			else
			{
				//alert(varstr);
				document.form1.submit();
			}

		}
	}
}

function Try2()
{
	var x;
	x=document.form1.VarName.length;
	var j=0;
	if (document.form1.AllVars.checked)
	{
		for (var i=0; i < x; i++) { j=i; document.form1.VarName[j].checked=true; }
	}
	else
	{
		for (var i=0; i < x; i++) {
			j=i;
			if (document.form1.VarName[j].disabled==false)
			{
				document.form1.VarName[j].checked=false;
			}
		 }
	}
	return true;
}

function Try2_2()
{
	var x=document.form1.GroupName.length;
	for (var i=0; i < x; i++)
	{
		var vString=document.form1.GroupName[i].value
		if (document.form1.AllGroups.checked)
		{
			var n=1
		}
		else
		{
			var n=0
		}
		document.form1.GroupName[i].checked=document.form1.AllGroups.checked;
		document.form1.GroupName[i].value=vString.substring(0,vString.length-1)+n;
		boxName="D_" + vString.substring(0,vString.length-1);
		if (document.getElementById(boxName))
		{
			document.getElementById(boxName).style.display="none";
		}
	}
}

function myreset()
{
	document.form1.reset();
}
function Try3(box)
{
	var x=document.form1.VarName.length;
	if (box.checked)
	{
		document.form1.AllVars.checked=true;
		document.form1.AllVars.disabled=true;
		document.form1.Download.style.display="none";
		document.form1.Download2.style.display="inline";
		document.form1.document.checked=true;
		document.form1.document.disabled=true;
		document.form1.TermsDef.checked=false;
		document.form1.TermsDef.disabled=true;
		for (var i=0; i < x; i++)
		{
			document.form1.VarName[i].checked=true;
			document.form1.VarName[i].disabled=true;
			document.form1.GEOGRAPHY.disabled=true;
		}

	}else{
		document.form1.AllVars.checked=false;
		document.form1.AllVars.disabled=false;
		document.form1.Download.style.display="inline";
		document.form1.Download2.style.display="none";
		document.form1.document.checked=false;
		document.form1.document.disabled=false;
		document.form1.TermsDef.disabled=false;
		for (var i=0; i < x; i++)
		{
			document.form1.VarName[i].checked=false;
			document.form1.VarName[i].disabled=false;
			document.form1.GEOGRAPHY.disabled=false;
		}
	}
}
function Try4(box)
{
	//alert(box.name);
	if (!box.checked)
	{
		document.form1.AllVars.checked=false;
	}

}

function Try4_2(box)
{
	var boxName=box.value.substring(0,box.value.length-1);
	if (box.checked)
	{
		box.value=boxName+1;
	}
	else
	{
		box.value=boxName+0;
	}

	if (document.form1[boxName])
	{

		var x=document.form1[boxName].length;
		if (!box.checked)
		{
			document.form1.AllGroups.checked=false;
			if (x)
			{
				for (var i=0; i < x; i++)
				{
					document.form1[boxName][i].checked=false;
				}
			}
			else
			{
				document.form1[boxName].checked=false;
			}
		}
		else
		{
			if (x)
			{
				for (var i=0; i < x; i++)
				{
					document.form1[boxName][i].checked=true;
				}
			}
			else
			{
				document.form1[boxName].checked=true;
			}
			var y=document.form1.GroupName.length;
			var allTrue=1;
			for (j=0;j<y;j++)
			{
				if (document.form1.GroupName[j].checked==false)
				{
					allTrue=0;
					break;
				}
			}
			if (allTrue==1)
			{
				document.form1.AllGroups.checked=true;
			}
		}
	}
	return true;
}

function Try4_3(box)
{
	var i=box.title-1;
	var boxName=box.id;
	var x=document.form1[boxName].length;
	var allFalse=1;
	var allTrue=1;
	vString=document.form1.GroupName[i].value
	if (!box.checked)
	{
		document.form1.AllGroups.checked=false;
		document.form1.GroupName[i].checked=false;
		allTrue=0;
		if (x)
		{
			for (j=0;j<x;j++)
			{
				if (document.form1[boxName][j].checked==true)
				{
					allFalse=0;
					break;
				}
			}
		}
		else
		{
			if (document.form1[boxName].checked==true)
			{
				allFalse=0;
			}
		}
	}
	else
	{
		allFalse=0;
		if (x)
		{
			for (j=0;j<x;j++)
			{
				if (document.form1[boxName][j].checked==false)
				{
					allTrue=0;
					break;
				}
			}
		}
		else
		{
			if (document.form1[boxName].checked==false)
			{
				allTrue=0;
			}
		}
	}
	if (allFalse==1)
	{
		n=0;
		document.form1.GroupName[i].checked=false;
		if (navigator.appName.substring(0,8)=="Netscape")
		{
			document.form1.GroupName[i].style.backgroundColor="#ffffff";
		}
		else
		{
			document.form1.GroupName[i].indeterminate=false;
		}
	}
	else if (allTrue==1)
	{
		n=1;
		document.form1.GroupName[i].checked=true;
		if (navigator.appName.substring(0,8)=="Netscape")
		{
			document.form1.GroupName[i].style.backgroundColor="#ffffff";
		}
		else
		{
			document.form1.GroupName[i].indeterminate=false;
		}
	}
	else
	{
		n=2;
		if (navigator.appName.substring(0,8)=="Netscape")
		{
			document.form1.GroupName[i].style.backgroundColor="#efefef";
		}
		else
		{
			document.form1.GroupName[i].indeterminate=true;
		}
	}
	document.form1.GroupName[i].value=vString.substring(0,vString.length-1)+n;
}

function ex_col(obj)
{
	var labelName=obj.title;
	var subName="D_" + labelName;
	x=document.form1[labelName].length;
	if (document.getElementById(subName))
	{
		if (document.getElementById(subName).style.display=="none")
		{
			document.getElementById(subName).style.display="";
			obj.style.color="purple";
		}
		else
		{
			document.getElementById(subName).style.display="None";
			obj.style.color="blue";
		}
		if (document.form1.AllGroups.checked)
		{
			if(x)
			{
				for (i=0;i<x;i++)
				{
					document.form1[labelName][i].checked=true;
				}
			}
			else
			{
				document.form1[labelName].checked=true;
			}
		}
		else
		{
			j=obj.htmlFor;
			vString=document.form1.GroupName[j].value;
			check=vString.charAt(vString.length-1);
			if (x)
			{
				for (i=0;i<x;i++)
				{
					if (check==1)
					{
						document.form1[labelName][i].checked=true;
					}
					else if (check==0)
					{
						document.form1[labelName][i].checked=false;
					}
				}
			}
			else
			{
				if (check==1)
				{
					document.form1[labelName].checked=true;
				}
				else if (check==0)
				{
					document.form1[labelName].checked=false;
				}
			}
		}
	}
}

function shNull(dObj)
{
	N=128;

	for (i=0;i<=N;i++)
	{
		shName="pctNull" + i;
		if (dObj.checked==true)
		{
			document.getElementById(shName).style.visibility="visible";
		}
		else
		{
			document.getElementById(shName).style.visibility="hidden";
		}
	}
}
function changeRegion(selectBox)
{	var selIndex = selectBox.selectedIndex;
	var selText = selectBox.options[selIndex].value;
	var tempstring=unescape(document.form1.sumRegion.value);
	var sumRArray=tempstring.split(";");
	with (document.form1.GEOGRAPHY)
	{
		options.length=0;
		var opIndex=0;
		for (var i = 0; i < sumRArray.length-1; i = i + 4)
		{
			if ((selText==sumRArray[i]) )
			{
				options[opIndex] = new Option(sumRArray[i+3],sumRArray[i+2]);
				opIndex++;
				last_item=sumRArray[i+3]
			}

		}

		if (last_item!="Not Applicable")
		{
			var allText="All " + document.form1.filter2.title + "s";
			options[opIndex]= new Option(allText,"All");
		}

	}
}

</script>
</html>
